<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
 
 
 <web-app>

<display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Add</servlet-name>
  	<display-name>Add</display-name>
  	<description></description>
  	<servlet-class>com.employee.add.Add</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SearchEmployee</servlet-name>
  	<display-name>SearchEmployee</display-name>
  	<description></description>
  	<servlet-class>com.employee.search.SearchEmployee</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ViewAll</servlet-name>
  	<display-name>ViewAll</display-name>
  	<description></description>
  	<servlet-class>com.employee.viewAll.ViewAll</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Update</servlet-name>
  	<display-name>Update</display-name>
  	<description></description>
  	<servlet-class>com.employee.update.Update</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Delete</servlet-name>
  	<display-name>Delete</display-name>
  	<description></description>
  	<servlet-class>com.employee.delete.Delete</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Add</servlet-name>
  	<url-pattern>/Add</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SearchEmployee</servlet-name>
  	<url-pattern>/Search</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ViewAll</servlet-name>
  	<url-pattern>/ViewAll</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Update</servlet-name>
  	<url-pattern>/Update</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Delete</servlet-name>
  	<url-pattern>/Delete</url-pattern>
  </servlet-mapping>
 
</web-app>


